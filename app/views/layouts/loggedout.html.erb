<!DOCTYPE html>
<html>
	
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
    <%= stylesheet_link_tag 'front'%>
  </head>
  
  <body>
  	<div class='front_container'>
		<div id="content">
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>"><%= value %></div>
			<% end %>
    		<%= yield %>
    	</div>
    </div>
  </body>
  
</html>